<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="anchor" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: blue;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.GUI.ChangeDeckControllerGUI">
   <children>
      <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="750.0" onMouseClicked="#back" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/return_bt.png" />
         </image>
      </ImageView>
      <Pane fx:id="deckPane" layoutY="50.0" prefHeight="550.0" prefWidth="420.0" />
      <ScrollPane fx:id="cardsPane" layoutX="450.0" prefHeight="600.0" prefWidth="300.0" />
      <Label fx:id="deckName" textFill="RED">
         <font>
            <Font name="Times New Roman" size="30.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
