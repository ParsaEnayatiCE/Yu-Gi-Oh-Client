<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" styleClass="root" stylesheets="@../css/setting.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.GUI.SettingControllerGUI">
   <children>
      <Label layoutX="191.0" layoutY="185.0" prefHeight="74.0" prefWidth="151.0" text="Volume:" textFill="WHITE">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <Button layoutX="209.0" layoutY="259.0" mnemonicParsing="false" onAction="#volumeUp" prefHeight="74.0" prefWidth="87.0" text="+">
         <font>
            <Font size="37.0" />
         </font>
      </Button>
      <Button layoutX="209.0" layoutY="337.0" mnemonicParsing="false" onAction="#volumeDown" prefHeight="74.0" prefWidth="87.0" text="-">
         <font>
            <Font size="37.0" />
         </font>
      </Button>
      <Button layoutX="209.0" layoutY="415.0" mnemonicParsing="false" onAction="#mute" prefHeight="74.0" prefWidth="87.0" text="Mute/Unmute">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
      <Button layoutX="663.0" layoutY="25.0" mnemonicParsing="false" onAction="#goBack" prefHeight="78.0" prefWidth="87.0" style="-fx-background-radius: 100;" text="Back">
         <font>
            <Font name="Stencil" size="21.0" />
         </font>
      </Button>
      <ImageView fitHeight="133.0" fitWidth="158.0" layoutX="505.0" layoutY="255.0" onMouseClicked="#surrender" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/surrender.jpg" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
   </children>
</AnchorPane>
